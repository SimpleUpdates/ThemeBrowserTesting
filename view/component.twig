<html class="{% if themeviz_use_bootstrap %}su_bootstrap_safe {% endif %}{{ themeviz_wrapper_classes }}">
<head>
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="{{ themeviz_base_path }}/third-party/iframe-resizer/js/iframeResizer.contentWindow.min.js"></script>
    {% if themeviz_use_bootstrap %}
        <link rel="stylesheet" href="{{ themeviz_base_path }}/third-party/bootstrap-3.3.5/dist/css/bootstrap.min.css">
        <script src="{{ themeviz_base_path }}/third-party/bootstrap-3.3.5/dist/js/bootstrap.min.js"></script>
    {% endif %}
    <style>
        {{ themeviz_css|raw }}

        .su_bootstrap_safe html,
        .su_bootstrap_safe body {
            height: auto;
            margin: 0;
            background-color: white;
        }
    </style>
</head>
<body class="{% if themeviz_use_bootstrap %}su_bootstrap_safe {% endif %}{{ themeviz_wrapper_classes }}">
{% include themeviz_component_path %}
</body>
</html>
